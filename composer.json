{
  "name": "someone/some-theme",
  "type": "project",
  "description": "Theme.",
	"license": "UNLICENSED",
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "phpcompatibility/php-compatibility": "*",
    "wp-coding-standards/wpcs": "*",
    "sirbrillig/phpcs-variable-analysis": "*",
    "wptrt/wpthemereview": "*"
  },
  "require": {
    "composer/installers": "*",
    "wpackagist-plugin/codepress-admin-columns": "*",
    "wpackagist-plugin/custom-post-type-ui": "*",
    "wpackagist-plugin/debug-bar": "*",
    "wpackagist-plugin/easy-theme-and-plugin-upgrades": "*",
    "wpackagist-plugin/enable-media-replace": "*",
    "wpackagist-plugin/favicon-by-realfavicongenerator": "*",
    "wpackagist-plugin/gutenberg": "*",
    "wpackagist-plugin/regenerate-thumbnails": "*",
    "wpackagist-plugin/scripts-n-styles": "*",
    "wpackagist-plugin/svg-support": "*",
    "wpackagist-plugin/theme-check": "*",
    "wpackagist-plugin/wordpress-beta-tester": "*",
    "wpackagist-plugin/wordpress-seo": "*",
    "wpackagist-plugin/pwa": "*"
  },
  "extra": {
    "installer-paths": {
      "dev/files/html/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "dev/files/html/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ]
}
